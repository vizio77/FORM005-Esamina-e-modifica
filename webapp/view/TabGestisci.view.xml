									</Input>
									<Input width="90%" editable="false" tooltip=" " value="{modelAnagraficaPf>/Descrestesamacroagg}"/>
								</form:content>
							</form:SimpleForm>
							<form:SimpleForm editable="true" labelSpanL="1" labelSpanM="1" labelSpanS="12" layout="ColumnLayout" columnsM="1"
