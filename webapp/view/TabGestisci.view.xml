												datbis="{ path: 'modelPageAut>/0/Datbis', formatter: '.formatter.formatterDatbis'}"/>
										</layout:HorizontalLayout>
									</layout:VerticalLayout>
									<layout:VerticalLayout class="sapUiTinyMarginBeginEnd">
