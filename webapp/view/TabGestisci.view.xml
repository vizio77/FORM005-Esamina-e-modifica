										</layoutData>
									</Input>
									<Input value="{modelAnagraficaPf>/Descrestragioneria}" width="90%" editable="false"/>
								</form:content>
							</form:SimpleForm>
							<form:SimpleForm editable="true" layout="ColumnLayout" labelSpanXL="1" labelSpanL="1" labelSpanM="1" labelSpanS="12" adjustLabelSpan="false"
