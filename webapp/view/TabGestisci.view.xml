								<columns>
									<Column id="colLiv1TabID" width="8em" demandPopin="true" vAlign="Middle" hAlign="Center">
										<Text text="{i18n>Liv1}"/>
									</Column>
									<Column id="colLiv2TabID" visible="true" width="8em" demandPopin="true" vAlign="Middle" hAlign="Center">
