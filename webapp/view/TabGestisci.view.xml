								</form:content>
							</form:SimpleForm>
							<form:SimpleForm editable="true" labelSpanL="1" labelSpanM="1" labelSpanS="12" layout="ColumnLayout" columnsM="1" columnsL="1" columnsXL="1">
								<form:content>
									<Label text="{i18n>Titolo}" required="true"/>
